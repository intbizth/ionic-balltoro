//TODO:h2h might be from another controller
.expand-collapse.away-collapse
    .header(ng-click!="active2 = !active2", ng-class!="{'collapsed': active2, 'expanded': !active2}")
        .caret
        | Away Team
    .slides(ng-hide="active2")
        .content
            .row.titles
                .col.col-15 Pos
                .col.col-25 Date
                .col.col-45
                .col.col-15 FT
        .div(ng-repeat="w in r")
            .row(ng-class-odd!="'odd'", ng-class-even!="'even'")
                .col.col-15 ww
                .col.col-25 ww
                .col.col-45 ww
                .col.col-15 ww
